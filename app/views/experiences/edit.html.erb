<div class="container-form">
  <H1>Edit an Experience</H1>
  <div class="form">
  <%= simple_form_for @experience do |f| %>
    <%= f.input :title %>
    <%= f.input :category %>
    <%= f.input :location %>
    <%= f.submit "Edit Experience", class: "btn btn-flat"%>
  <% end %>
  </div>
  <%= link_to "Go back", experiences_path, class: "btn btn-ghost"%>
</div>
