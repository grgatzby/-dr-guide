<div class="container-form">
  <H1>Create an Experience</H1>
  <div class="form">
  <%= simple_form_for @experience do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :category %>
    <%= f.input :location %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.submit "Create Experience", class: "btn btn-flat" %>
  <% end %>
  </div>
  <%= link_to "Go back", experiences_path, class: "btn btn-ghost"%>
</div>
